type Country {
  CountryCode: String!
  ShortName: String!
  TableName: String!
  LongName: String!
  Alpha2Code: String!
  CurrencyUnit: String!
  SpecialNotes: String!
  Region: String!
  IncomeGroup: String!
}

type Query {
  hello: String!
  countries(search: String ,limit: Int = 10, offset: Int = 10): [Country!]!
}